{
    "name": "drjele/utility-phpunit",
    "type": "symfony-bundle",
    "description": "Library used to make writing phpunit tests easier",
    "keywords": [
        "phpunit"
    ],
    "homepage": "https://gitlab.com/drjele-utility/phpunit",
    "license": "proprietary",
    "authors": [
        {
            "name": "Constantin Adrian Jeledintan",
            "email": "adrian.jeledintan@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "mockery/mockery": "^1.4",
        "symfony/phpunit-bridge": "^4.1|^5.1",
        "symfony/error-handler": "^4.1|^5.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.3",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "repositories": [
    ],
    "autoload": {
        "psr-4": {
            "Drjele\\Utility\\Phpunit\\": "src/"
        }
    },
    "scripts": {
        "install-hooks": [
            "[ $COMPOSER_DEV_MODE -eq 1 ] && rm -rf .git/hooks && ln -s ../scripts/git_hooks .git/hooks || exit 0"
        ],
        "post-install-cmd": [
            "@install-hooks"
        ],
        "post-update-cmd": [
            "@install-hooks"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "exclude": [
        ]
    }
}
